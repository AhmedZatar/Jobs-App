<%- include('layout/head') %>

    <body>
        <%- include('layout/header') %>
            <main>
                <h1>All USA Jobs</h1>
                <div class="jobs">
                <%jobs.forEach((job)=>{%>

              

                    <div>
                        <h2>Title: <%=job.title%></h2>
                        <h2>Company: <%=job.company%></h2>
                        <h2>Location: <%=job.location%></h2>
                        <a href="<%=job.url%>"><%=job.url%></a>
                    </div>


                 <% }) %>

                </div>
            </main>

    </body>

    </html>